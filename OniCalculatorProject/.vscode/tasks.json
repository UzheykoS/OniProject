{
	"version": "0.1.0",
	"command": "npm",
	"isShellCommand": true,
	"suppressTaskName": true,
	"args": [
		"--display-modules",
		"--progress",
		"--colors"
	],
	"showOutput": "always",
	"tasks": [
		{
			"args": ["run-script", "webpack-watch"],
			"taskName": "webpack_watch"
		},
		{
			"args": ["run-script", "webpack_dev"],
			"taskName": "webpack_single"
		}
	]
}